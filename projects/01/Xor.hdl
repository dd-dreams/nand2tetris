/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    	Or(a=a, b=b, out=tmp);
	Nand(a=a, b=b, out=tmp1);
	And(a=tmp, b=tmp1, out=out);
}
